<?xml version="1.0"?>
<extension key="primaryaddress" type="module">
  <file>primaryaddress</file>
  <name>primaryaddress</name>
  <description>If a user has an address of type "User entered online", make that the primary address if the only other address location type is "Billing".</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Brienne Kordis</author>
    <email>brienne@megaphonetech.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-09-05</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.61</ver>
  </compatibility>
  <comments>This is s custom extension, made and maintained by Megaphone Technology.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Primaryaddress</namespace>
    <format>23.02.1</format>
    <angularModule>crmPrimaryaddress</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
